    "#Simulação da fila com as entradas e saídas da base de dados\n",
    "\n",
    "\n",
    "def fila(env, recurso, paciente, saida, chegada, prioridade, saiuFila):\n",
    "    yield env.timeout(chegada)\n",
    "    yield env.timeout(saida)\n",
    "    yield env.timeout(saiuFila)\n",
    "    if saiuFila != 0:\n",
    "        print(paciente, 'saiu da fila em', saiuFila)\n",
    "    else:\n",
    "        with recurso.request(priority=prioridade) as req:\n",
    "            yield req\n",
    "            print(\"Paciente:\", paciente, 'atendida em', chegada)\n",
    "            \n",
    "            print(paciente, 'saiu do atendimento em', saida)\n",
    "            yield env.timeout(saida)\n",
    "\n",
    "env = simpy.Environment()\n",
    "recurso = simpy.PriorityResource(env, capacity=1)\n",
    "\n",
    "for i in range(100):\n",
    "    env.process(fila(env, recurso, idPaciente100[i], tempo100[i], dataChegada100[i], 0, 0))\n",
    "\n",
    "\n",
    "# env.process(fila(env, recurso, 'Maria', 6, 0, 0, 0))\n",
